<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.piyu004.contactdetails">

    <uses-permission android:name="android.permission.READ_CONTACTS"></uses-permission>
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name=".SQLExample"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="com.example.piyu004.contactdetails.SQLEXAMPLE" />
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name=".SQLView"
            android:label="@string/app_name" >
        -<intent-filter>
            <action android:name="com.example.piyu004.contactdetails.SQLView"/>
            <category android:name="android.intent.category.DEFAULT"/>
    </intent-filter>
        </activity>
    </application>
</manifest>
